<body>

<h1>Editing Book</h1>

<%= form_with model:@book do |f| %>
<div class="field">
  <ul>title</ul>
  <%= f.text_field:title %>
</div>
<div class="field">
  <ul>body</ul>
  <%= f.text_area:body %>
</div>
<div class="field">
<%= f.submit'Update Book' %>
</div>
<ul>
<%= link_to "Show",book_path(@book.id) %>
|
<%= link_to "Back",books_path %>
</ul>
<% end %>

</body>